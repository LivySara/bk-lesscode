@import "@/css/mixins/scroller";

.table-row-actions {
    display: flex;
    align-items: center;
    flex-wrap: wrap;
    .action-btn {
        margin-right: 8px;
    }
}
.table-row-detail {
    padding: 24px;
    height: calc(100vh - 60px);
    overflow: auto;
    @mixin scroller;
    .bk-label {
        font-size: 12px;
    }
}
